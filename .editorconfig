# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
indent_size = 2
end_of_line = lf
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = false

[*.{js,css,sass,scss,json,vue,html}]
indent_style = space
indent_size = 2

[*.php]
indent_size = 4
